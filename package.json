{
  "name": "streamfi-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "setup-db": "node scripts/setup-db.cjs",
    "update-schema": "node scripts/updateUserSchema.mjs",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "prepare": "husky",
    "commit": "git-cz",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rm -rf .next out coverage",
    "prebuild": "npm run type-check",
    "predev": "npm run type-check"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/inter": "^5.1.1",
    "@hookform/resolvers": "^5.1.1",
    "@livepeer/react": "^4.3.5",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@neondatabase/serverless": "^1.0.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-email/render": "^1.0.6",
    "@starknet-react/chains": "^3.1.2",
    "@starknet-react/core": "^3.7.2",
    "@starknet-react/typescript-config": "^0.0.1",
    "@vercel/postgres": "^0.10.0",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.6.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "embla-carousel-react": "^8.5.2",
    "formidable": "^3.5.4",
    "framer-motion": "^12.5.0",
    "hls.js": "^1.6.5",
    "input-otp": "^1.4.2",
    "livepeer": "^3.5.0",
    "lucide": "^0.475.0",
    "lucide-react": "^0.475.0",
    "mongoose": "^8.14.1",
    "motion": "^12.4.7",
    "next": "^14.2.29",
    "nodemailer": "^6.10.1",
    "nprogress": "^0.2.0",
    "pg": "^8.15.6",
    "react": "^18",
    "react-dom": "^18",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.60.0",
    "react-icons": "^5.5.0",
    "sharp": "^0.33.5",
    "sonner": "^2.0.2",
    "starknet": "^6.24.1",
    "swiper": "^11.2.4",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/cors": "^2.8.19",
    "@types/formidable": "^3.4.5",
    "@types/jest": "^30.0.0",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "husky": "^9.1.7",
    "jest-environment-jsdom": "^30.0.4",
    "lint-staged": "^16.1.5",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
